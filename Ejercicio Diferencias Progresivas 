      "cell_type": "code",
      "execution_count": 4,
      "metadata": {
        "id": "YE0K8vI8ay9o"
      },
      "outputs": [],
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd"
      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "Inciso a)\n",
        "h=0.1 y k=0.0005"
      ],
      "metadata": {
        "id": "7gwlCzW3It4D"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "alpha = 1.0\n",
        "h = 0.1\n",
        "k = 0.0005\n",
        "T = 0.5\n",
        "L = 1.0\n",
        "\n",
        "# Se nombra como \"lambda\" al cociente de (alpha*k)/h^2\n",
        "lambda_ = alpha * k / h**2\n",
        "print(f\"λ = {lambda_:.3f} (debe ser ≤ 0.5 para estabilidad)\")\n",
        "\n",
        "x = np.arange(0, L + h, h)\n",
        "nt = int(T / k)\n",
        "\n",
        "# Condición inicial\n",
        "u = np.sin(np.pi * x)\n",
        "u_new = np.zeros_like(u)\n",
        "\n",
        "for n in range(nt):\n",
        "    for i in range(1, len(x) - 1):\n",
        "        u_new[i] = u[i] + (lambda_) * (u[i+1] - 2*u[i] + u[i-1])\n",
        "    u_new[0] = 0\n",
        "    u_new[-1] = 0\n",
        "    u[:] = u_new[:]\n",
        "\n",
        "# Solución exacta\n",
        "u_exact = np.exp(-np.pi**2 * alpha * T) * np.sin(np.pi * x)\n",
        "\n",
        "# Tabla de comparación entre solución numérica y solución analítica\n",
        "tabla = pd.DataFrame({\n",
        "    'x': x,\n",
        "    'u_numérica': u,\n",
        "    'u_exacta': u_exact,\n",
        "    'Error absoluto': np.abs(u - u_exact)\n",
        "})\n",
        "\n",
        "print(\"\\nComparación numérica vs exacta en t = 0.5:\\n\")\n",
        "print(tabla.to_string(index=False))\n",
        "\n",
        "# Gráfica\n",
        "plt.plot(x, u, 'ro-', label='Numérica')\n",
        "plt.plot(x, u_exact, 'k--', label='Exacta')\n",
        "plt.title(f\"Ecuación del calor 1D (h={h}, k={k}, T={T})\")\n",
        "plt.xlabel(\"x\")\n",
        "plt.ylabel(\"u(x,T)\")\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
